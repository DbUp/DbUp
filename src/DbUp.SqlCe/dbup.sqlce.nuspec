<?xml version="1.0" encoding="utf-8"?> 
<package> 
  <metadata> 
    <id>dbup-sqlce</id> 
    <title>DbUp SQL CE Support</title>
    <version>$version$</version> 
    <authors>Paul Stovell, Jim Burger, Jake Ginnivan, Damian Maclennan</authors>
    <description>DbUp makes it easy to deploy and upgrade SQL Server databases. This package adds SqlCe support.</description> 
    <language>en-US</language>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>http://dbup.github.com</projectUrl>
    <iconUrl>https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png</iconUrl>
    <dependencies>
      <dependency id="Microsoft.SqlServer.Compact" version="4.0.8876.1" />
      <dependency id="dbup" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\DbUp.SqlCe.dll" target="lib\net40\" />
    <file src="bin\$configuration$\DbUp.SqlCe.pdb" target="lib\net40\" />
    <file src="bin\$configuration$\DbUp.SqlCe.xml" target="lib\net40\" />
  </files>
</package>
