{
    "version": "1.0.0-*",
    "title": "DbUp PostgreSQL Support",
    "description": "DbUp makes it easy to deploy and upgrade SQL Server databases. This package adds PostgreSQL support.",
    "authors": [ "Paul Stovell", "Jim Burger", "Jake Ginnivan", "Damian Maclennan" ],
    "packOptions": {
        "projectUrl": "http://dbup.github.com",
        "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
        "releaseNotes": "https://github.com/DbUp/DbUp/releases",
        "iconUrl": "https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png"
    },
    "buildOptions": {
        "keyFile": "../dbup.snk"
    },

    "dependencies": {
        "dbup-core": { "target": "project" },
        "Npgsql": "3.2.1"
    },

    "frameworks": {
        "netstandard1.3": {
            "imports": "dnxcore50",
            "dependencies": {
              "NETStandard.Library": "1.6.1",
              "System.Data.Common": "4.3.0",
              "System.Linq.Expressions": "4.3.0",
              "System.Reflection": "4.3.0",
              "System.Reflection.TypeExtensions": "4.3.0"
            },
            "net40": {
      
            }
      }
    }
}
