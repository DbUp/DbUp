<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>dbup-mysql</id>
    <title>DbUp MySql Support</title>
    <version>$version$</version>
    <authors>Paul Stovell, Jim Burger, Jake Ginnivan, Damian Maclennan</authors>
    <description>DbUp makes it easy to deploy and upgrade SQL Server databases. This package adds MySql support.</description>
    <language>en-US</language>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>http://dbup.github.com</projectUrl>
    <iconUrl>https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png</iconUrl>
    <dependencies>
      <dependency id="MySql.Data" version="6.9.5" />
      <dependency id="dbup" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\DbUp.MySql.dll" target="lib\net35\" />
    <file src="bin\$configuration$\DbUp.MySql.pdb" target="lib\net35\" />
    <file src="bin\$configuration$\DbUp.MySql.xml" target="lib\net35\" />
  </files>
</package>
