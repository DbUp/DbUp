{
    "version": "1.0.0-*",
    "title": "DbUp MySql Support",
    "description": "DbUp makes it easy to deploy and upgrade SQL Server databases. This package adds MySql support.",
    "authors": [ "Paul Stovell", "Jim Burger", "Jake Ginnivan", "Damian Maclennan" ],
    "packOptions": {
        "projectUrl": "http://dbup.github.com",
        "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
        "releaseNotes": "https://github.com/DbUp/DbUp/releases",
        "iconUrl": "https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png"
    },
    "buildOptions": {
        "keyFile": "../dbup.snk"
    },

    "dependencies": {
        "dbup-core": { "target": "project" },
        "MySql.Data": "6.9.5"
    },

    "frameworks": {
        "net40": {
            "frameworkAssemblies": {
              "System.Data": "4.0.0.0"
            }
        }
    }
}
