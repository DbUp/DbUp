<?xml version="1.0" encoding="utf-8"?> 
<package> 
  <metadata> 
    <id>dbup-firebird</id> 
    <title>DbUp Firebird Support</title>
    <version>$version$</version> 
    <authors>Paul Stovell, Jim Burger, Jake Ginnivan, Damian Maclennan, Jesper Niedermann</authors>
    <description>DbUp makes it easy to deploy and upgrade SQL Server databases. This package adds Firebird support.</description> 
    <language>en-US</language>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <projectUrl>http://dbup.github.com</projectUrl>
    <iconUrl>https://raw.github.com/DbUp/DbUp/master/src/Information/dbup-icon.png</iconUrl>
    <dependencies>
        <dependency id="FirebirdSql.Data.FirebirdClient" version="4.5.2.0" />
        <dependency id="dbup" version="$version$" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\DbUp.Firebird.dll" target="lib\net40\" />
    <file src="bin\$configuration$\DbUp.Firebird.pdb" target="lib\net40\" />
    <file src="bin\$configuration$\DbUp.Firebird.xml" target="lib\net40\" />
  </files>
</package>
